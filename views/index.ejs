<!--Head including title (home, products, about, project, contact) and CSS section <body>-->
<%- include partials/header.ejs %>

<!--All parts of Nav Bar (home, products, about, project, contact)-->
<%- include partials/mainnav.ejs %>

<!--Main Content added Jumbotron-->
<main class="container">
  <div class="jumbotron">
    
<!--Using Switch case all infornation passed through here-->
      <% switch (title) {
        case 'Home' : %>
        <%- include partials/home.ejs %>
                <% break;

        case 'About' : %>
        <%- include partials/about.ejs %>
                <% break;

        case 'Product' : %>
        <%- include partials/Product.ejs %>
                <% break;
        
        case 'Services' : %>
        <%- include partials/services.ejs %>
                <% break;
        
        case 'Contact' : %>
        <%- include partials/contact.ejs %>
              <% break;
       } %>
      <hr class="my-4">
    </div>
  </main>

  <!--Bottom Nav Footer-->
  <%- include partials/bottomnav.ejs %>

  <!--Script Section </body>-->
  <%- include partials/footer.ejs %>